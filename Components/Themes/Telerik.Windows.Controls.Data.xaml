  <ControlTemplate TargetType="telerik:RadButton" x:Key="PageButtonTemplate">
    <Grid SnapsToDevicePixels="True">
      <Ellipse x:Name="Border" MinHeight="22" MinWidth="22" Fill="{telerik:Windows8Resource ResourceKey=MainBrush}" Stretch="Fill" Stroke="{telerik:Windows8Resource ResourceKey=BasicBrush}" StrokeThickness="1" />
