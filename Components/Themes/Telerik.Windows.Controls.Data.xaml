      <Grid x:Name="ContentGrid">
        <ContentPresenter x:Name="Content" Opacity="0.5" Content="{TemplateBinding Content}" Margin="{TemplateBinding Padding}" ContentTemplate="{TemplateBinding ContentTemplate}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
          <ContentPresenter.RenderTransform>
            <TransformGroup>
              <ScaleTransform />
              <SkewTransform />
              <RotateTransform />
              <TranslateTransform />
            </TransformGroup>
          </ContentPresenter.RenderTransform>
        </ContentPresenter>
      </Grid>
      <!-- Focus -->
      <Border x:Name="CommonStatesWrapper">
        <Ellipse x:Name="FocusVisual" Visibility="Collapsed" Opacity="0" MinHeight="22" MinWidth="22" StrokeThickness="1" Stroke="{telerik:Windows8Resource ResourceKey=MarkerBrush}" />
      </Border>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="IsMouseOver" Value="true">
        <Setter TargetName="Border" Property="Fill" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}" />
        <Setter TargetName="Content" Property="Opacity" Value="1" />
      </Trigger>
      <Trigger Property="IsPressed" Value="True">
        <Setter TargetName="Border" Property="Fill" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}" />
        <Setter TargetName="Content" Property="Opacity" Value="1" />
        <Setter TargetName="Content" Property="RenderTransform">
          <Setter.Value>
            <TranslateTransform X="1" />
          </Setter.Value>
        </Setter>
      </Trigger>
      <Trigger Property="IsEnabled" Value="False">
        <Setter TargetName="Content" Property="Opacity" Value="0.3" />
        <Setter TargetName="Border" Property="Opacity" Value="0.3" />
      </Trigger>
      <Trigger Property="IsFocused" Value="True">
        <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
        <Setter TargetName="FocusVisual" Property="Opacity" Value="0.2" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
