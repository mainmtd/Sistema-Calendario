    <ControlTemplate x:Key="DropDownItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="Mouse">
                    <VisualState x:Name="MouseEnter">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="headerContainer" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
