          </Track.Thumb>
        </Track>
        <RepeatButton Style="{StaticResource RepeatButtonLineDownStyle}" Grid.Row="2" Command="{x:Static ScrollBar.LineDownCommand}" />
      </Grid>
    </Border>
  </ControlTemplate>
  <Style x:Key="ScrollBarStyle" TargetType="{x:Type ScrollBar}">
